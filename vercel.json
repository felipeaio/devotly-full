{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "routes": [
    { "src": "https://www.devotly.shop/(.*)", "status": 308, "headers": { "Location": "https://devotly.shop/$1" } },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/success", "dest": "/frontend/success.html" },
    { "src": "/failure", "dest": "/frontend/error.html" },
    { "src": "/pending", "dest": "/frontend/pending.html" },
    { "src": "/create", "dest": "/frontend/create/create.html" },
    { "src": "/edit", "dest": "/frontend/edit/edit.html" },
    { "src": "/view", "dest": "/frontend/view/view.html" },
    { "src": "/js/(.*)", "dest": "/frontend/js/$1" },
    { "src": "/css/(.*)", "dest": "/frontend/css/$1" },
    { "src": "/home", "dest": "/frontend/home.html" },
    { "handle": "filesystem" },
    { "src": "/(.*\\.(?:json|png|ico|xml|svg|webp))", "dest": "/frontend/$1" },
    { "src": "/", "dest": "/frontend/home.html" }
  ]
}
